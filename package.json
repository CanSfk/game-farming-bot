{
  "name": "game-farming-bot",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "src/main.ts",
  "bin": "build/main.js",
  "scripts": {
    "build": "tsup",
    "copy-json": "cp -R ./src/jsons ./bin",
    "start": "tsup && node build/main.js",
    "pkg": "tsup && pkg .",
    "test": "echo \"Error: no test specified\" && exit 1"

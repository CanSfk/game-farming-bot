  "pkg": {
    "scripts": "build/main.js",
    "assets": "jsons/**/*.json",
    "targets": [
      "node18-win-x64",
      "node18-macos-arm64"
    ],
    "outputPath": "bin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "robotjs": "^0.6.0",
    "undici-types": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
